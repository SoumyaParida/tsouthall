ofile-website/tsouthall/src/theme/typography.tsx"],"names":["sansSerifFont","monospaceFont","baseFontSize","baseFontWeight","typography","paragraphFontSize","paragraphFontSizeMobile","paragraphFontFamily","paragraphFontWeight","paragraphLetterSpacing","paragraphLineHeight","footerLinkFontSize","footerLinkFontWeight","footerLinkFontFamily","sloganFontSize","sloganFontWeight","sloganFontFamily","logoFontFamily","menuFontSize","menuFontFamily","menuFontWeight","menuFontWeightMobile","menuFontWeightActive","addressFontSize","addressFontFamily","titleFontSize","titleFontSizeMobile","titleFontFamily","titleFontWeight","titleLetterSpacing","subtitleFontSize","subtitleFontSizeMobile","subtitleFontFamily","subtitleFontWeight","subtitleFontWeightMobile","subtitleLetterSpacing","accordionFontSize","accordionFontFamily","accordionFontWeight","accordionLineHeight","accordionLetterSpacing"],"mappings":"AAAA,IAAMA,aAAa,GAAG,kBAAtB;AACA,IAAMC,aAAa,GAAG,sBAAtB;AACA,IAAMC,YAAY,GAAG,EAArB;AACA,IAAMC,cAAc,GAAG,GAAvB;AACA,IAAMC,UAAU,GAAG;AAClBC,EAAAA,iBAAiB,YAAKH,YAAL,OADC;AAElBI,EAAAA,uBAAuB,YAAKJ,YAAY,GAAG,CAApB,OAFL;AAGlBK,EAAAA,mBAAmB,EAAEP,aAHH;AAIlBQ,EAAAA,mBAAmB,EAAEL,cAJH;AAKlBM,EAAAA,sBAAsB,EAAE,QALN;AAMlBC,EAAAA,mBAAmB,YAAKR,YAAY,GAAGA,YAAY,GAAG,CAAnC,OAND;AAOlBS,EAAAA,kBAAkB,YAAKT,YAAY,GAAG,CAApB,OAPA;AAQlBU,EAAAA,oBAAoB,EAAET,cARJ;AASlBU,EAAAA,oBAAoB,EAAEb,aATJ;AAUlBc,EAAAA,cAAc,YAAKZ,YAAL,OAVI;AAWlBa,EAAAA,gBAAgB,EAAEZ,cAXA;AAYlBa,EAAAA,gBAAgB,EAAEhB,aAZA;AAalBiB,EAAAA,cAAc,EAAEjB,aAbE;AAclBkB,EAAAA,YAAY,YAAKhB,YAAY,GAAG,CAApB,OAdM;AAelBiB,EAAAA,cAAc,EAAEnB,aAfE;AAgBlBoB,EAAAA,cAAc,EAAEjB,cAAc,GAAG,GAhBf;AAiBlBkB,EAAAA,oBAAoB,EAAElB,cAjBJ;AAkBlBmB,EAAAA,oBAAoB,EAAEnB,cAAc,GAAG,GAlBrB;AAmBlBoB,EAAAA,eAAe,YAAKrB,YAAY,GAAG,CAApB,OAnBG;AAoBlBsB,EAAAA,iBAAiB,EAAExB,aApBD;AAqBlByB,EAAAA,aAAa,YAAKvB,YAAL,OArBK;AAsBlBwB,EAAAA,mBAAmB,YAAKxB,YAAY,GAAG,EAApB,OAtBD;AAuBlByB,EAAAA,eAAe,EAAE3B,aAvBC;AAwBlB4B,EAAAA,eAAe,EAAEzB,cAAc,GAAG,GAxBhB;AAyBlB0B,EAAAA,kBAAkB,EAAE,QAzBF;AA0BlBC,EAAA